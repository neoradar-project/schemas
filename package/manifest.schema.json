{
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "version": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "namespace": {
      "type": "string"
    },
    "createdAt": {
      "type": "string"
    },
    "centerPoint": {
      "type": "array",
      "items": {
        "type": "number"
      }
    },
    "mapLayers": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "type": {
            "type": "string",
            "enum": [
              "geojson"
            ]
          },
          "source": {
            "type": "string",
            "default": "auto"
          },
          "defaultStyle": {
            "type": "object",
            "properties": {
              "lineStyle": {
                "type": "object",
                "properties": {
                  "color": {
                    "type": "array",
                    "items": {
                      "type": "number"
                    },
                    "minItems": 3,
                    "maxItems": 3,
                    "description": "RGB color array [r,g,b]"
                  },
                  "width": {
                    "type": "number",
                    "default": 1
                  },
                  "opacity": {
                    "type": "number",
                    "default": 1
                  },
                  "dashArray": {
                    "type": "array",
                    "items": {
                      "type": "number"
                    }
                  }
                },
                "required": [
                  "color"
                ],
                "additionalProperties": false
              },
              "fillStyle": {
                "type": "object",
                "properties": {
                  "color": {
                    "$ref": "#/properties/mapLayers/items/properties/defaultStyle/properties/lineStyle/properties/color"
                  },
                  "opacity": {
                    "type": "number",
                    "default": 1
                  }
                },
                "required": [
                  "color"
                ],
                "additionalProperties": false
              },
              "symbolStyle": {
                "type": "object",
                "properties": {
                  "symbol": {
                    "type": "string"
                  },
                  "size": {
                    "type": "number",
                    "default": 10
                  }
                },
                "required": [
                  "symbol"
                ],
                "additionalProperties": false
              },
              "textStyle": {
                "type": "object",
                "properties": {
                  "text": {
                    "type": "string"
                  },
                  "color": {
                    "$ref": "#/properties/mapLayers/items/properties/defaultStyle/properties/lineStyle/properties/color",
                    "description": "RGB color array [r,g,b]"
                  },
                  "fontFamily": {
                    "type": "string",
                    "default": "RobotoMono-Regular"
                  },
                  "fontSize": {
                    "type": "number",
                    "default": 12
                  },
                  "fontWeight": {
                    "type": [
                      "string",
                      "number"
                    ],
                    "enum": [
                      "normal",
                      "bold",
                      "bolder",
                      "lighter",
                      100,
                      200,
                      300,
                      400,
                      500,
                      600,
                      700,
                      800,
                      900
                    ],
                    "default": "normal"
                  },
                  "size": {
                    "type": "number",
                    "default": 10
                  },
                  "offset": {
                    "type": "array",
                    "items": {
                      "type": "number"
                    },
                    "minItems": 2,
                    "maxItems": 2,
                    "default": [
                      0,
                      0
                    ]
                  },
                  "anchor": {
                    "type": "string",
                    "enum": [
                      "start",
                      "middle",
                      "end"
                    ],
                    "default": "middle"
                  },
                  "baseline": {
                    "type": "string",
                    "enum": [
                      "top",
                      "middle",
                      "bottom",
                      "alphabetic"
                    ],
                    "default": "middle"
                  },
                  "letterSpacing": {
                    "type": "number",
                    "default": 0
                  },
                  "lineHeight": {
                    "type": "number",
                    "default": 0
                  },
                  "align": {
                    "type": "string",
                    "enum": [
                      "left",
                      "center",
                      "right"
                    ],
                    "default": "center"
                  },
                  "wordWrap": {
                    "type": "boolean",
                    "default": false
                  },
                  "wordWrapWidth": {
                    "type": "number",
                    "default": 100
                  },
                  "outlineColor": {
                    "$ref": "#/properties/mapLayers/items/properties/defaultStyle/properties/lineStyle/properties/color",
                    "description": "RGB color array [r,g,b]"
                  },
                  "outlineWidth": {
                    "type": "number",
                    "default": 0
                  },
                  "outlineOpacity": {
                    "type": "number",
                    "default": 1
                  },
                  "dropShadow": {
                    "type": "boolean",
                    "default": false
                  },
                  "dropShadowColor": {
                    "$ref": "#/properties/mapLayers/items/properties/defaultStyle/properties/lineStyle/properties/color",
                    "description": "RGB color array [r,g,b]",
                    "default": [
                      0,
                      0,
                      0
                    ]
                  },
                  "dropShadowAngle": {
                    "type": "number",
                    "default": 0.5235987755982988
                  },
                  "dropShadowBlur": {
                    "type": "number",
                    "default": 0
                  },
                  "dropShadowDistance": {
                    "type": "number",
                    "default": 5
                  },
                  "dropShadowOpacity": {
                    "type": "number",
                    "default": 1
                  },
                  "scaleWithZoom": {
                    "type": "boolean",
                    "default": true
                  },
                  "minZoom": {
                    "type": "number"
                  },
                  "maxZoom": {
                    "type": "number"
                  },
                  "padding": {
                    "type": "number",
                    "default": 0
                  },
                  "backgroundColor": {
                    "$ref": "#/properties/mapLayers/items/properties/defaultStyle/properties/lineStyle/properties/color",
                    "description": "RGB color array [r,g,b]"
                  },
                  "backgroundOpacity": {
                    "type": "number",
                    "default": 0
                  }
                },
                "additionalProperties": false
              },
              "uuid": {
                "type": "string"
              },
              "type": {
                "type": "string"
              },
              "label": {
                "type": "string"
              },
              "value": {
                "type": "string"
              },
              "_mappedName": {
                "type": "string"
              }
            },
            "required": [
              "uuid"
            ],
            "additionalProperties": false
          },
          "hasLabels": {
            "type": "boolean"
          },
          "isLabelLayer": {
            "type": "boolean"
          }
        },
        "required": [
          "name",
          "type"
        ],
        "additionalProperties": false
      }
    }
  },
  "required": [
    "id",
    "name",
    "namespace",
    "createdAt",
    "centerPoint"
  ],
  "additionalProperties": false,
  "$schema": "http://json-schema.org/draft-07/schema#"
}