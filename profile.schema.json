{
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "type": {
      "type": "string",
      "enum": [
        "situation",
        "profile"
      ]
    },
    "updatedAt": {
      "type": "string"
    },
    "modals": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "x": {
            "type": "number"
          },
          "y": {
            "type": "number"
          },
          "shown": {
            "type": "boolean"
          }
        },
        "required": [
          "x",
          "y",
          "shown"
        ],
        "additionalProperties": false
      }
    },
    "radarData": {
      "type": "object",
      "properties": {
        "gndFilterStatus": {
          "type": "boolean"
        },
        "primFilterStatus": {
          "type": "boolean"
        },
        "vfrFilterStatus": {
          "type": "boolean"
        },
        "upperFilterValue": {
          "type": "number"
        },
        "lowerFilterValue": {
          "type": "number"
        }
      },
      "additionalProperties": false
    },
    "atmSystem": {
      "type": "string"
    },
    "map": {
      "type": "object",
      "properties": {
        "center": {
          "type": "object",
          "properties": {
            "x": {
              "type": "number"
            },
            "y": {
              "type": "number"
            }
          },
          "required": [
            "x",
            "y"
          ],
          "additionalProperties": false
        },
        "zoom": {
          "type": "number"
        },
        "orientation": {
          "type": "number"
        },
        "windowArea": {
          "type": "object",
          "properties": {
            "minX": {
              "type": "number"
            },
            "minY": {
              "type": "number"
            },
            "maxX": {
              "type": "number"
            },
            "maxY": {
              "type": "number"
            }
          },
          "required": [
            "minX",
            "minY",
            "maxX",
            "maxY"
          ],
          "additionalProperties": false
        },
        "items": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "showLabel": {
                "type": "boolean"
              },
              "uuid": {
                "type": "string"
              }
            },
            "required": [
              "showLabel",
              "uuid"
            ],
            "additionalProperties": false
          }
        }
      },
      "required": [
        "center",
        "zoom",
        "items"
      ],
      "additionalProperties": false
    },
    "filePath": {
      "type": "string"
    },
    "lastModified": {
      "type": "number"
    }
  },
  "required": [
    "name",
    "type",
    "map"
  ],
  "additionalProperties": false,
  "description": "A profile containing a view state displaying radar map items",
  "$schema": "http://json-schema.org/draft-07/schema#"
}