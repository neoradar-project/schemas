{
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "type": {
      "type": "string",
      "enum": [
        "situation",
        "profile"
      ]
    },
    "updatedAt": {
      "type": "string"
    },
    "modals": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "x": {
            "type": "number"
          },
          "y": {
            "type": "number"
          },
          "shown": {
            "type": "boolean"
          }
        },
        "required": [
          "x",
          "y",
          "shown"
        ],
        "additionalProperties": false
      }
    },
    "radarData": {
      "type": "object",
      "properties": {
        "gndFilterStatus": {
          "type": "boolean",
          "default": true
        },
        "primFilterStatus": {
          "type": "boolean",
          "default": true
        },
        "vfrFilterStatus": {
          "type": "boolean",
          "default": true
        },
        "upperFilterValue": {
          "type": "number"
        },
        "lowerFilterValue": {
          "type": "number"
        },
        "speedVector": {
          "type": "object",
          "properties": {
            "value": {
              "type": "number"
            },
            "type": {
              "type": "string",
              "enum": [
                "NM",
                "MIN"
              ]
            }
          },
          "required": [
            "value",
            "type"
          ],
          "additionalProperties": false
        },
        "transitionAltitude": {
          "type": "number"
        }
      },
      "additionalProperties": false
    },
    "atmSystem": {
      "type": "string"
    },
    "map": {
      "type": "object",
      "properties": {
        "center": {
          "type": "object",
          "properties": {
            "x": {
              "type": "number"
            },
            "y": {
              "type": "number"
            }
          },
          "required": [
            "x",
            "y"
          ],
          "additionalProperties": false
        },
        "zoom": {
          "type": "number"
        },
        "orientation": {
          "type": "number"
        },
        "windowArea": {
          "type": "object",
          "properties": {
            "minX": {
              "type": "number"
            },
            "minY": {
              "type": "number"
            },
            "maxX": {
              "type": "number"
            },
            "maxY": {
              "type": "number"
            }
          },
          "required": [
            "minX",
            "minY",
            "maxX",
            "maxY"
          ],
          "additionalProperties": false
        },
        "items": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "showLabel": {
                "type": "boolean"
              },
              "uuid": {
                "type": "string"
              }
            },
            "required": [
              "showLabel",
              "uuid"
            ],
            "additionalProperties": false
          }
        }
      },
      "required": [
        "center",
        "zoom",
        "items"
      ],
      "additionalProperties": false
    },
    "filePath": {
      "type": "string"
    },
    "lastModified": {
      "type": "number"
    },
    "folderTree": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "relatedProfiles": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "relatedLoginProfiles": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "cursors": {
      "type": "object",
      "properties": {
        "global": {
          "type": "string"
        },
        "map": {
          "type": "string"
        },
        "qdmTool": {
          "type": "string"
        },
        "distanceTool": {
          "type": "string"
        },
        "headingTool": {
          "type": "string"
        },
        "routeDraw": {
          "type": "string"
        },
        "vectorDraw": {
          "type": "string"
        },
        "rangeRings": {
          "type": "string"
        },
        "directTo": {
          "type": "string"
        },
        "pointAndClick": {
          "type": "string"
        },
        "pointer": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "source": {
      "type": "string",
      "enum": [
        "local",
        "package"
      ]
    },
    "_saveMetadata": {
      "type": "object",
      "properties": {
        "source": {
          "type": "string"
        },
        "operationId": {
          "type": "string"
        }
      },
      "required": [
        "operationId"
      ],
      "additionalProperties": false
    }
  },
  "required": [
    "name",
    "type",
    "map"
  ],
  "additionalProperties": false,
  "description": "A profile containing a view state displaying radar map items",
  "$schema": "http://json-schema.org/draft-07/schema#"
}