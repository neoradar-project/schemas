{
  "type": "object",
  "properties": {
    "$schema": {
      "type": "string"
    },
    "groundSymbology": {
      "type": "object",
      "properties": {
        "default": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "description": {
                "type": "string"
              },
              "applyWhen": {
                "anyOf": [
                  {
                    "type": "string"
                  },
                  {
                    "anyOf": [
                      {
                        "anyOf": [
                          {
                            "type": "array",
                            "minItems": 3,
                            "maxItems": 3,
                            "items": [
                              {
                                "type": "string",
                                "enum": ["==", "!=", "<", "<=", ">", ">=", "beginsWith", "endsWith", "contains"]
                              },
                              {
                                "anyOf": [
                                  {
                                    "type": "string",
                                    "enum": [
                                      "stopped",
                                      "timestamp",
                                      "verticalSpeed",
                                      "verticalTrend",
                                      "altitude",
                                      "agl",
                                      "onGround",
                                      "groundSpeed",
                                      "reportedHeading",
                                      "trackHeading",
                                      "transponder",
                                      "latLon",
                                      "trueAltitude",
                                      "pressureAltitude"
                                    ]
                                  },
                                  {
                                    "type": "string",
                                    "enum": [
                                      "flightRule",
                                      "rawType",
                                      "icaoTransponderEquipment",
                                      "icaoEquipment",
                                      "icaoWakeCategory",
                                      "acShortType",
                                      "origin",
                                      "destination",
                                      "alternate",
                                      "plannedTas",
                                      "plannedFlightLevel",
                                      "rmk",
                                      "flightTimeHours",
                                      "flightTimeMinutes",
                                      "fuelTimeHours",
                                      "fuelTimeMinutes",
                                      "eobt",
                                      "aobt",
                                      "isValid",
                                      "voiceType"
                                    ]
                                  },
                                  {
                                    "type": "string",
                                    "enum": ["callsign", "type", "isTimingOut", "lastPositionUpdateReceivedAt", "squawk", "latestTransponderStatus"]
                                  },
                                  {
                                    "type": "string",
                                    "enum": [
                                      "clearedFlightLevel",
                                      "assignedHeading",
                                      "assignedSpeed",
                                      "assignedMach",
                                      "assignedVerticalRate",
                                      "clearanceIssued",
                                      "assignedSquawk",
                                      "ownedByMe",
                                      "ownedByCallsign",
                                      "scratchpad",
                                      "groundStatus",
                                      "clearanceQueuePosition",
                                      "attentionState"
                                    ]
                                  },
                                  {
                                    "type": "string",
                                    "enum": ["sid", "star", "isAmended", "depRunway", "arrRunway", "rawRoute", "hadStar", "hadSid"]
                                  },
                                  {
                                    "type": "string",
                                    "enum": [
                                      "isArrival",
                                      "isDeparture",
                                      "isMyDeparture",
                                      "isMyArrival",
                                      "suggestedStar",
                                      "suggestedSid",
                                      "suggestedDepRunway",
                                      "suggestedArrRunway",
                                      "myCallsign",
                                      "ownedByID",
                                      "computedSpeed",
                                      "computedCFL",
                                      "text"
                                    ]
                                  }
                                ]
                              },
                              {
                                "type": ["string", "number"]
                              }
                            ]
                          },
                          {
                            "type": "array",
                            "minItems": 2,
                            "maxItems": 2,
                            "items": [
                              {
                                "type": "string",
                                "enum": ["AND", "OR", "NOT"]
                              },
                              {
                                "type": "array",
                                "items": {
                                  "anyOf": [
                                    {
                                      "$ref": "#/properties/groundSymbology/properties/default/items/properties/applyWhen/anyOf/1/anyOf/0"
                                    },
                                    {
                                      "type": "string",
                                      "pattern": "^\\$[A-z0-9_-]+"
                                    }
                                  ]
                                }
                              }
                            ]
                          },
                          {
                            "type": "array",
                            "minItems": 3,
                            "maxItems": 3,
                            "items": [
                              {
                                "$ref": "#/properties/groundSymbology/properties/default/items/properties/applyWhen/anyOf/1/anyOf/0/anyOf/1/items/0"
                              },
                              {
                                "type": "string",
                                "enum": ["notEmpty", "isEmpty"]
                              },
                              {
                                "$ref": "#/properties/groundSymbology/properties/default/items/properties/applyWhen/anyOf/1/anyOf/0/anyOf/0/items/1"
                              }
                            ]
                          },
                          {
                            "type": "array",
                            "minItems": 2,
                            "maxItems": 2,
                            "items": [
                              {
                                "$ref": "#/properties/groundSymbology/properties/default/items/properties/applyWhen/anyOf/1/anyOf/0/anyOf/2/items/1"
                              },
                              {
                                "$ref": "#/properties/groundSymbology/properties/default/items/properties/applyWhen/anyOf/1/anyOf/0/anyOf/0/items/1"
                              }
                            ]
                          }
                        ]
                      },
                      {
                        "$ref": "#/properties/groundSymbology/properties/default/items/properties/applyWhen/anyOf/1/anyOf/0/anyOf/1/items/1/items/anyOf/1"
                      }
                    ]
                  }
                ]
              },
              "applyToAttentionStates": {
                "type": "array",
                "items": {
                  "type": "string",
                  "enum": ["kUnconcerned", "kNotified", "kIncomingTransfer", "kAssumed", "kOutgoingTransfer", "kIntruder"]
                }
              },
              "plotImage": {
                "oneOf": [
                  {
                    "type": "string"
                  },
                  {
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  }
                ],
                "default": ""
              },
              "historyTrailImages": {
                "type": "array",
                "items": {
                  "type": "string"
                },
                "default": []
              },
              "speedVectorColor": {
                "anyOf": [
                  {
                    "type": "array",
                    "items": {
                      "type": "number"
                    },
                    "minItems": 3,
                    "maxItems": 3
                  },
                  {
                    "type": "string",
                    "pattern": "^[&=][A-z0-9_-]+"
                  },
                  {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "pattern": "^[&=][A-z0-9_-]+"
                    }
                  }
                ]
              },
              "rotatePlotWithHeading": {
                "type": "boolean",
                "default": false
              },
              "rotateHistoryTrailWithHeading": {
                "type": "boolean",
                "default": false
              },
              "historyTrailsSkipSteps": {
                "type": "number",
                "default": 0
              }
            },
            "required": ["applyWhen"],
            "additionalProperties": false
          }
        },
        "arrival": {
          "type": "array",
          "items": {
            "$ref": "#/properties/groundSymbology/properties/default/items"
          }
        },
        "departure": {
          "type": "array",
          "items": {
            "$ref": "#/properties/groundSymbology/properties/default/items"
          }
        }
      },
      "required": ["default", "arrival", "departure"],
      "additionalProperties": false
    },
    "airborneSymbology": {
      "type": "array",
      "items": {
        "$ref": "#/properties/groundSymbology/properties/default/items"
      }
    }
  },
  "required": ["$schema", "groundSymbology", "airborneSymbology"],
  "additionalProperties": false,
  "$schema": "http://json-schema.org/draft-07/schema#"
}
